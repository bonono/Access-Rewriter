{
	"name": "Access Rewriter",
	"version": "0.1",
	"manifest_version": 2,
	"icons": {
		"16" : "icon.png",
		"48" : "icon.png"
	},
	"browser_action" : {
		"default_icon" : {
			"19" : "icon.png",
			"38" : "icon.png"
		},
		"default_title" : "Access Rewriter",
		"default_popup" : "html/popup.html"
	},
	"options_page": "html/option.html",
	"background": {
		"scripts": [ 
			"script/js/config.js",
			"script/js/background.js"
		]
	},
	"permissions": [
		"storage",
		"unlimitedStorage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	]
}